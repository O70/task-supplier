<?xml version="1.0" encoding="UTF-8"?>
<!--
    OFF	0
    FATAL	100
    ERROR	200 default
    WARN	300
    INFO	400
    DEBUG	500
    TRACE	600
    ALL	Integer.MAX_VALUE
-->
<Configuration>
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"/>
        </Console>
    </Appenders>
    <Loggers>
        <!--<Logger name="org.thraex.supplier" level="trace">-->
            <!--<AppenderRef ref="Console"/>-->
        <!--</Logger>-->
        <Root>
            <AppenderRef ref="Console"/>
        </Root>
    </Loggers>
</Configuration>
